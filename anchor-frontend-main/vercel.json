{
  "rewrites": [
    {
      "source": "/auth-proxy/:path*",
      "destination": "http://92.4.78.222/auth-server/:path*"
    },
    {
      "source": "/api/auth/:path*",
      "destination": "http://92.4.78.222/auth-server/:path*"
    },
    {
      "source": "/api/backend/:path*",
      "destination": "http://92.4.78.222/api/v1/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}